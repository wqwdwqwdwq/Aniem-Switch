local Player = game.Players.LocalPlayer
local HRP = game.Players.LocalPlayer.Character.HumanoidRootPart
local P 
_G.Valorr = true
	local BodyPosition = Instance.new("BodyVelocity")
			


while _G.Valorr  do 
	wait()
	if Player.Character:FindFirstChildWhichIsA("Humanoid").Health <= 20 then
		_G.Valorr = false
		while not _G.Valorr do 
			wait()
			Player.Character.HumanoidRootPart.CFrame =  CFrame.new(0,40,0)
			if Player.Character:FindFirstChildWhichIsA("Humanoid").Health >= 30 then
				_G.Valorr = true
			end
		end
	end
for _, Npcs in pairs(game.Workspace.Alive:GetChildren()) do

		

		if Npcs ~= game.Players.LocalPlayer.Character and Npcs:IsA("Model") and Npcs.Name ~= "comandobr540" then
			
			Player.Character.HumanoidRootPart.CFrame = Npcs.HumanoidRootPart.CFrame * CFrame.new(0,-2,0)

		end
	end
end


_G.Valor = false
